<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="245.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.ProjectTeamMembersViewController">
    <children>
        <HBox prefHeight="33.0" prefWidth="600.0">
            <children>
                <TextField fx:id="ProjectID">
                    <HBox.margin>
                        <Insets left="10.0" top="6.0" />
                    </HBox.margin>
                </TextField>
                <Label prefHeight="35.0" prefWidth="231.0" text="Project ID">
                    <font>
                        <Font size="24.0" />
                    </font>
                    <HBox.margin>
                        <Insets left="20.0" />
                    </HBox.margin>
                </Label>
            </children>
            <VBox.margin>
                <Insets top="10.0" />
            </VBox.margin>
        </HBox>
        <HBox prefHeight="100.0" prefWidth="200.0">
            <children>
                <TextArea fx:id="teamMemberList" prefHeight="106.0" prefWidth="200.0" promptText="Name">
                    <HBox.margin>
                        <Insets left="10.0" />
                    </HBox.margin>
                </TextArea>
                <Label text="Team Members List">
                    <font>
                        <Font size="24.0" />
                    </font>
                    <HBox.margin>
                        <Insets left="10.0" top="25.0" />
                    </HBox.margin>
                </Label>
            </children>
            <opaqueInsets>
                <Insets />
            </opaqueInsets>
            <VBox.margin>
                <Insets top="10.0" />
            </VBox.margin>
        </HBox>
        <Label fx:id="errorLabel" text="errorLabel" textFill="RED">
            <VBox.margin>
                <Insets left="10.0" top="5.0" />
            </VBox.margin>
        </Label>
        <HBox prefHeight="26.0" prefWidth="450.0">
            <children>
                <Button mnemonicParsing="false" onAction="#backButtonPressed" text="Back">
                    <font>
                        <Font size="14.0" />
                    </font>
                    <HBox.margin>
                        <Insets left="10.0" />
                    </HBox.margin>
                </Button>
                <Button mnemonicParsing="false" onAction="#submitButtonPressed" text="Submit">
                    <font>
                        <Font size="14.0" />
                    </font>
                    <HBox.margin>
                        <Insets left="20.0" />
                    </HBox.margin>
                </Button>
            </children>
            <VBox.margin>
                <Insets top="20.0" />
            </VBox.margin>
        </HBox>
    </children>
</VBox>
